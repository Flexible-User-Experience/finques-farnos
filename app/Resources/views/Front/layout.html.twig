<!DOCTYPE html>
<!--[if lt IE 7]>     <html class="no-js lt-ie9 lt-ie8 lt-ie7"><![endif]-->
<!--[if IE 7]>        <html class="no-js lt-ie9 lt-ie8"><![endif]-->
<!--[if IE 8]>        <html class="no-js lt-ie9"><![endif]-->
<!--[if gt IE 8]><!--><html class="no-js"><!--<![endif]-->
<head>
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]-->
    <title>{% block title %}Finques Farnós{% endblock %}</title>
    <meta name="description" content="{% block metadescription %}{% endblock %}">
    <meta name="keywords" content="{% block metakeywords %}{% endblock %}">
    <meta name="author" content="www.flux.cat">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/x-icon" href="/favicon.ico">
    <link rel="shortcut icon" type="image/png" href="/favicon.png">
    {% block extrametas %}{% endblock %}
    {% block stylesheets %}
        <link rel="stylesheet" href="/css/main.css">
    {% endblock %}
    {% block javascriptshead %}
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        <script src="/js/main.min.js"></script>
    {% endblock %}
</head>
<body {% block bodyattr %}{% endblock %}>
<!--[if lt IE 9]>
<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->
{#<!-- Fixed top menu -->#}
<nav class="navbar navbar-default ff" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" data-aria-expanded="false" data-aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('front_homepage') }}"><img src="{{ asset('bundles/app/images/logo-ff.png') }}" alt="Finques Farnós"/></a>
            <p class="navbar-text text-hide">Finques Farnós</p>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
            {{ knp_menu_render('frontend', {'currentClass': 'active'}) }}
        </div>
    </div>
</nav>
{#<!-- Main content -->#}
{% block main %}{% endblock %}
{#<!-- Footer -->#}
<footer>
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <address>
                    <strong>FINQUES FARNÓS, SL</strong><br>
                    C. Corsini, 61<br>
                    43870 Amposta (Tarragona)<br>
                    Espanya<br>
                    (+34) 977 702 721
                </address>
            </div>
            <div class="col-md-6 text-center">
                <img src="{{ asset('bundles/app/images/logo-footer-api.png') }}" alt="API Número col·legiat 379 Cinta Farnós i Bel"/>
                <img src="{{ asset('bundles/app/images/logo-footer-rm.png') }}" alt="Registre Agents immobiliaris de Catalunya aicat 2237"/>
            </div>
            <div class="col-md-3">
                <div class="row">
                    <div class="col-md-12">
                        <div class="btn-group pull-right" role="group" data-aria-label="languageSelector">
                            {% if app.request.locale != 'ca' %}<a href="{% block footlangca %}{{ path('front_homepage', {'_locale': 'ca'}) }}{% endblock %}" class="btn btn-default btn-sm">català</a>{% endif %}
                            {% if app.request.locale != 'es' %}<a href="{% block footlanges %}{{ path('front_homepage', {'_locale': 'es'}) }}{% endblock %}" class="btn btn-default btn-sm">español</a>{% endif %}
                            {% if app.request.locale != 'en' %}<a href="{% block footlangen %}{{ path('front_homepage', {'_locale': 'en'}) }}{% endblock %}" class="btn btn-default btn-sm">english</a>{% endif %}
                            {% if app.request.locale != 'fr' %}<a href="{% block footlangfr %}{{ path('front_homepage', {'_locale': 'fr'}) }}{% endblock %}" class="btn btn-default btn-sm">français</a>{% endif %}
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <ul class="list-inline pull-right">
                            <li><small><a href="{{ path('front_privacy', {'_locale': app.request.locale}) }}">{{ 'frontend.privacy' | trans }}</a></small></li>
                            <li><small><a href="{{ path('front_legal', {'_locale': app.request.locale}) }}">{{ 'frontend.legal' | trans }}</a></small></li>
                            <li><small><a href="{{ path('front_credits', {'_locale': app.request.locale}) }}">{{ 'frontend.credits' | trans }}</a></small></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</footer>
{#<!-- Google Analytics -->#}
{% if google_analytics_enabled %}
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        ga('create', '{{ google_analytics_account }}', 'auto'); ga('send', 'pageview');
    </script>
{% endif %}
{% block javascriptsbody %}{% endblock %}
</body>
</html>
