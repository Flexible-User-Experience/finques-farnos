{% extends 'Front/layout.html.twig' %}

{% block title %}{{ 'menu.properties' | trans }} · {{ parent() }} · {{ 'homepage.property.banner.metatitle' | trans }}{% endblock %}
{% block metadescription %}{{ parent() }}{% endblock %}
{% block metakeywords %}{{ parent() }}{% endblock %}

{% block main %}
    <div class="container">
        <div class="row no-m-bottom">
            <div class="col-md-12">
                <h1 class="c-orange no-m-botton properties-list-h1"><span class="search-icon"></span>{{ 'properties.heading.title' | trans | upper }}</h1>
            </div>
        </div>
        <div class="row extra-m-bottom-30">
            <div class="col-md-12">
                <iframe src="{{ get_inmopc_iframe_src_general() }}" frameborder="0" width="100%" height="1375"></iframe>
            </div>
        </div>
        <hr class="hr-homepage">
    </div>
    {# Scroll-Top effect #}
    <div class="scroll-top-wrapper"><span class="scroll-top-inner"><i class="fa fa-2x fa-arrow-circle-up"></i></span></div>
{% endblock %}

{% block javascriptsbody %}
    {% include ':Front/commons:scroll-to-top-js.html.twig' %}
{% endblock %}

{% block footlangca %}{{ path('front_properties_inmopc', {_locale: 'ca'}) }}{% endblock %}
{% block footlanges %}{{ path('front_properties_inmopc', {_locale: 'es'}) }}{% endblock %}
{% block footlangen %}{{ path('front_properties_inmopc', {_locale: 'en'}) }}{% endblock %}
{% block footlangfr %}{{ path('front_properties_inmopc', {_locale: 'fr'}) }}{% endblock %}
