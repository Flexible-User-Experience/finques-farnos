{
    "name": "symfony/framework-standard-edition",
    "license": "proprietary",
    "type": "project",
    "description": "Finques Farnos webapp project",
    "autoload": {
      "psr-4": {
        "": "src/",
        "SymfonyStandard\\": "app/SymfonyStandard/"
      }
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/migrations": "1.*",
        "doctrine/doctrine-migrations-bundle": "1.*",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "twig/extensions": "~1.0",

        "a2lix/translation-form-bundle": "1.*",
        "cocur/slugify": "1.*",
        "components/jquery": "2.1.*",
        "craue/formflow-bundle": "2.1.*",
        "cypresslab/gitelephant-bundle": "dev-develop",
        "egulias/email-validator": "1.2.*",
        "endroid/qrcode-bundle": "1.*",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "hautelook/alice-bundle": "0.*",
        "ircmaxell/password-compat": "1.0.*",
        "jms/i18n-routing-bundle": "1.*",
        "liip/functional-test-bundle": "1.2.*",
        "liip/imagine-bundle": "1.*",
        "mopa/bootstrap-bundle": "3.x-dev",
        "ob/highcharts-bundle": "1.*",
        "orkestra/pdf-bundle": "1.0.x-dev",
        "sonata-project/admin-bundle": "2.3.*",
        "sonata-project/datagrid-bundle": "2.2.x-dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.*",
        "sonata-project/user-bundle": "2.3.x-dev",
        "stof/doctrine-extensions-bundle": "1.*",
        "tecnick.com/tcpdf": "6.*",
        "twbs/bootstrap": "3.3.*",
        "vich/uploader-bundle": "0.*",

        "raulfraile/ladybug-bundle": "1.*",
        "oh/google-map-form-type-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "2.*",
        "presta/sitemap-bundle": "1.3.*",
        "friendsofsymfony/rest-bundle": "1.4.*",
        "nelmio/api-doc-bundle": "2.7.*",
        "genemu/form-bundle": "2.2.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.6-dev"
        }
    }
}
